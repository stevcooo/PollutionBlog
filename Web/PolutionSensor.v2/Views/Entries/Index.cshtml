@model IEnumerable<PolutionSensor.v2.Models.Entry>

@{
    ViewBag.Title = "PolutionSensor Entries";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pm10)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pm25)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SSID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsertDateTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pm10)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pm25)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SSID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocalDateTime)
            </td>
        </tr>
    }

</table>
